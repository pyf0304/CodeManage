            <span class="navbar-toggler-icon"></span>
        </button>
        <div>
            <div class="navbar-collapse collapse" id="navbarSupportedContent2" style="width:60%; float:left">
                <ul class="navbar-nav mr-auto">
                    @*<li class="nav-item" style="width:200px;">   <a asp-page="/Index" class="navbar-brand">界面信息维护</a></li>*@

                    <li class="nav-item active"><a asp-page="/PrjInterface/WApiViewInfo_QUDI_TS">界面列表</a></li>

                    <li class="nav-item ml-2"><a asp-page="/RegionManage/ViewRegionInTab">界面区域维护</a></li>
                    <li class="nav-item ml-2"><a asp-page="/PrjInterface/WApiViewRelaTab_QUDI_TS">界面相关表维护</a></li>
                    <li class="nav-item ml-2"><a asp-page="/RegionManage/WApiQryRegionFlds_QUDI_TS">查询区</a></li>
                    <li class="nav-item ml-2"><a asp-page="/RegionManage/WApiEditRegionFlds_QUDI_TS">编辑区</a></li>
