    @RenderSection("Scripts", required: false)
    <script>
        var strWebRoot = "@Model.WebRoot";
        var strUserLoginInfo = "@Model.seObjUserLoginInfo";
        var strViewInfo4Session = "@Model.seobjViewInfo4Session";

        /*
 * 页面导入-在导入页面后运行的函数
*/
        function DispUserInfo() {
            require(["../js/Index/Main_ViewInfo.js"], function (main) {
                main.Main_ViewInfo.DispUserInfo();
            });
        }
        DispUserInfo();

        function DispViewInfo() {
            require(["../js/Index/Main_ViewInfo.js"], function (main) {
                main.Main_ViewInfo.DispViewInfo();
            });
        }
        DispViewInfo();
