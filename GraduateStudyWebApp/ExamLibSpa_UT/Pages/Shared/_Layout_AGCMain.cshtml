<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - AGC</title>

    @RenderSection("Styles", required: false)
</head>
<body>

    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        

        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent2" aria-controls="navbarSupportedContent2" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div style="width:100%; float:left">
            <ul class="navbar-nav">
                <li class="nav-item" style="width:200px;">   <a asp-page="/Index" class="navbar-brand">自动生成代码</a></li>

                <li class="nav-item">用户Id:</li>
                <li class="nav-item"><span id="spnUserId"></span></li>
                <li class="nav-item ml-2">角色:</li>
                <li class="nav-item"><span id="spnRoleName"></span></li>
                <li class="nav-item ml-2">项目:</li>
                <li class="nav-item"><span id="spnCurrSelPrjId"></span></li>
                <li class="nav-item">(<span id="spnCurrSelPrjName"></span>)</li>
                <li class="nav-item ml-2">数据库:</li>
                <li class="nav-item"><span id="spnPrjDataBaseName"></span></li>
            </ul>
        </div>
        <div class="navbar-collapse collapse" id="navbarSupportedContent2" style="width:100%; float:left">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item active"><a asp-page="/Index">Home</a></li>

                <li class="nav-item ml-2"><a asp-page="/PrjInterface/WApiViewInfo_QUDI_TS">界面信息维护</a></li>

                <li class="nav-item ml-2"><a asp-page="/Table_Field/WApiPrjTab_QUDI_TS">工程表维护</a></li>
                <li class="nav-item ml-2"><a asp-page="/GeneCode/GeneCode4Tab">生成表代码</a></li>

                <li class="nav-item ml-2"><a asp-page="/GeneCode/TestGeneCode">测试生成代码</a></li>



            </ul>

        </div>
    </nav>
    <partial name="_CookieConsentPartial" />

    <div class=" body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; 2019 - AGC</p>
        </footer>
    </div>
      
    @RenderSection("Scripts", required: false)
    <script>
        var strWebRoot = "@Model.WebRoot";
        var strUserLoginInfo = "@Model.seObjUserLoginInfo";

        /*
 * 页面导入-在导入页面后运行的函数
*/
        function DispUserInfo() {
            require(["../js/Index/Main.js"], function (main) {
                main.Main.DispUserInfo();
            });
        }
        DispUserInfo();
    </script>
</body>
</html>
