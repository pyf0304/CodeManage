
@page
@{
Layout = "";
}
@section Styles{
    <link rel="stylesheet" href="../lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="../css/site.css" />
}
@section Scripts{
    <script src="../lib/jquery/dist/jquery.js"></script>
    <script src="../lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="../js/site.js" asp-append-version="true"></script>
    <script src="../lib/bootstrap/dist/js/popper.js"></script>
    <script src="../lib/require/require.js" data-main="../js/src/config"></script>
}
     <script>    

 /*
  显示对话框
 (AutoGCLib.WA_ViewScript_Edit_TS4Html:Gen_WApi_JS_ShowDialog)
 */
function ShowDialog_CacheUseState(pstrOp) {
   strOp = pstrOp;
   require(['jquery', 'bootstrap'], function($) {
       if (strOp === "Add") {
           $('#lblDialogTitle_CacheUseState').html('添加记录');
//CacheUseState_EditEx.GetMaxStrId('#txtmId');
        }
else if (strOp === "Update")
{
            $('#lblDialogTitle_CacheUseState').html('修改记录');
}
else if (strOp === "Detail")
{
            $('#btnSubmit_CacheUseState').hide();
            $('#lblDialogTitle_CacheUseState').html('详细信息');
}
        $('#divEditDialog_CacheUseState').modal('show');
  });
}

 /*
  隐藏对话框
 (AutoGCLib.WA_ViewScript_Edit_TS4Html:Gen_WApi_JS_HideDialog)
 */
function HideDialog_CacheUseState() {
  require(['jquery', 'bootstrap'], function($) {
      $('#divEditDialog_CacheUseState').modal('hide');
  });
}
    </script>
<div id = "tabLayout" class = "tab_layout" 
>
@*-- 编辑层 --*@

 <div class="modal fade" id="divEditDialog_CacheUseState" tabindex="-1" role="dialog" aria-labelledby="lblDialogTitle_CacheUseState" aria-hidden="true">
 <div class="modal-dialog">
 <div class="modal-content" style="width: 635px;">
 <div class="modal-header">
 <h4 class="modal-title" id="lblDialogTitle_CacheUseState">模态框（Modal）标题</h4>
 <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
 </div>
 <div class="modal-body">
<table id="tabEdit" style="width:600px" class="table table-bordered table-hover table td table-sm">
<tr>
<td class="text-right">
<Label id="lblCacheModeId" name="lblCacheModeId" class="col-form-label text-right" style="width:90px;">缓存方式Id
</Label>
</td>
<td class="text-left">
<select id="ddlCacheModeId" name="ddlCacheModeId" class="form-control" style="width:300px;"></select>
</td>
</tr>
<tr>
<td class="text-right">
<Label id="lblCacheKey" name="lblCacheKey" class="col-form-label text-right" style="width:90px;">缓存关键字
</Label>
</td>
<td class="text-left">
<input  id="txtCacheKey" name="txtCacheKey" class="form-control" style="width:300px;"/>
</td>
</tr>
<tr>
<td class="text-right">
<Label id="lblCacheSize" name="lblCacheSize" class="col-form-label text-right" style="width:90px;">缓存大小
</Label>
</td>
<td class="text-left">
<input  id="txtCacheSize" name="txtCacheSize" class="form-control" style="width:300px;"/>
<asp:CompareValidator id="ComValid_CacheSize" name="ComValid_CacheSize" runat="server" CssClass="ErrMsg"
 ErrorMessage="请输入整型数值!"
 ControlToValidate="txtCacheSize"
 Type="Integer"
 Operator="DataTypeCheck"
 EnableClientScript="false">
</asp:CompareValidator>
</td>
</tr>
<tr>
<td class="text-right">
<Label id="lblMemo" name="lblMemo" class="col-form-label text-right" style="width:90px;">说明
</Label>
</td>
<td class="text-left">
<input  id="txtMemo" name="txtMemo" class="form-control" style="width:300px;"/>
</td>
</tr>
</table>
 </div>
 <div class="modal-footer">
 <button  id="btnCancel_CacheUseState" type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
 <button  id="btnSubmit_CacheUseState" type="button" class="btn btn-primary" onclick="Submit_CacheUseState()">添加</button>
 </div>
 </div>
 <!-- /.modal-content -->
 </div>
 <!-- /.modal -->
 </div>
</div>