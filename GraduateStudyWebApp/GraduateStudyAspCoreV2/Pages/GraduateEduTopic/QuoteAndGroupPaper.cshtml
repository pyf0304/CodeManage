      //上传方法
      function doUpload() {
        var formData = new FormData($("#uploadForm")[0]);
        $.ajax({
             url: "@Url.Action("UploadFile", "testUploadPic")",
            type: 'post',
            data: formData,
            async: false,
            cache: false,
            contentType: false,
            processData: false,
            success: function (returndata) {
                //成功/为空
                if (returndata.status == 0 || returndata.status == -1) {

                    //如果地址为空，那么是修改 则不用存放地址；
                    if (returndata.fileName != "") {
                        $("#hdnpic").val(returndata.fileName);
                    }


                    require(["../js/GraduateEduTopic/QuoteAndGroupPaperEx.js"], function (paper) {
                        var objPage = new paper.QuoteAndGroupPaperEx();
                        objPage.btnOKUpd_Click();
                    });
                    // var aa = JSON.stringify(returndata);

                    //alert(returndata.message);

                }
                else {

                    alert('请选择正确文件!(in QuoteAndGroupPaper.doUpload)')
                }


            },
            error: function (returndata) {
                alert(returndata);
            }
        });
    }
