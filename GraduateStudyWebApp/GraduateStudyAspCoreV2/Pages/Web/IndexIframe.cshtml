<script>
    //清空框架页
    function RemoveIfame_Click() {
        localStorage.removeItem('hid')
        $('.layui-tab-title li[lay-id]').find('.layui-tab-close').click();
    }

    //接收的参数
    function GetRequest() {
        var url = location.search; //获取url中"?"符后的字串
        var theRequest = new Object();
        if (url.indexOf("?") != -1) {
            var str = url.substr(1);
            strs = str.split("&");
            for (var i = 0; i < strs.length; i++) {
                theRequest[strs[i].split("=")[0]] = unescape(strs[i].split("=")[1]);
            }
        }
        return theRequest;
    }
    //加载首页
    window.onload = function () {
        //加载页面的时候关闭控制框架的所有缓存页面
        RemoveIfame_Click();

        var Request = new Object();
        Request = GetRequest();
        var str1 = Request['page'];

        $("#hidPage").val(str1);

        require(["../js/Web/IndexIframe.js"], function (index) {
            var objPage = new index.IndexIframe();
            objPage.Page_Load();
        });
    }

    //加载答疑统计数量
    setInterval('Bind_QuestionsCount()', 300000);  //指定300s刷新一次

    function Bind_QuestionsCount() {
        require(["../js/Web/IndexIframe.js"], function (index) {
            var objPage = new index.IndexIframe();
            objPage.Bind_QuestionsCount();
        });
    }
    //切换教学班
    function EduCls_Click(strkey, strName, strTypeID) {
        require(["../js/Web/IndexIframe.js"], function (index) {
            var objPage = new index.IndexIframe();
            objPage.EduCls_Click(strkey, strName, strTypeID);
        });
        //RemoveIfame_Click();
    }

    //加载欢迎页
    //function WelcomeLoad_Click() {
    //    require(["../js/Web/Welcome.js"], function (welcome) {
    //        var objPage = new welcome.Welcome();
    //        objPage.Page_Load();
    //    });
    //}

    //信息提示
    function layui_Alert(iconKey, strMsg) {

        layer.msg(strMsg, {
            icon: iconKey,
            time: 2000
        });
    }

</script>
