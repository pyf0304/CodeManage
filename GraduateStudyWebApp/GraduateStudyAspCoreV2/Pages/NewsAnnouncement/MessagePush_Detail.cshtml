 /**
  隐藏对话框
 (AutoGCLib.WA_ViewScript_Detail_TS4Html:Gen_WApi_JS_HideDialog)
 **/
function HideDialog(strDialogName) {
  require(['jquery', 'bootstrap'], function($) {
      $(strDialogName).modal('hide');
  });
}

 /**
  显示对话框
 (AutoGCLib.WA_ViewScript_Detail_TS4Html:Gen_WApi_JS_ShowDialog)
 **/
function ShowDialog(strDialogName) {
   require(['jquery', 'bootstrap'], function($) {
 $(strDialogName).modal('show');
  });
}
    </script>
